<?xml version="1.0"?>
<ivy-module xmlns:m="http://ant.apache.org/ivy/maven" version="2.0">
    <info organisation="org.jimwebber" module="neo4j-koans"/>
    <dependencies>
        <dependency org="org.neo4j.app" name="neo4j-server" rev="1.5">
            <artifact name="neo4j-server" ext="jar"/>
            <artifact name="neo4j-server" m:classifier="static-web" ext="jar"/>
            <artifact name="neo4j-server" m:classifier="tests" ext="jar"/>
			<exclude org="org.slf4j" name="slf4j-log4j12"/>
        </dependency>
        <dependency org="org.neo4j" name="neo4j-community" rev="1.5"/>
        <dependency org="org.neo4j" name="neo4j-kernel" rev="1.5">
            <artifact name="neo4j-kernel" ext="jar"/>
