        <dependency org="org.mockito" name="mockito-all" rev="1.8.5"/>
        <dependency org="junit" name="junit" rev="4.7"/>
        <dependency org="org.slf4j" name="slf4j-api" rev="1.6.1"/>
        <dependency org="org.slf4j" name="slf4j-jdk14" rev="1.6.1"/>
